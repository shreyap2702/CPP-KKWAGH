#include<iostream>
using namespace std;

class add
{
 private:
         int a ,b,sum;
        
 public:
         int data()
         {
          cout<<"Enter two No:"<<endl;
          cin>>a>>b;
          return 0;
        
         }
         
         int add1()
         {
          sum=a+b;
          
          cout<<"Sum : "<<sum<<endl;
          return 0;
         }
};          
           
int main()
{
 add obj;
 obj.data();
 obj.add1();
 
 return 0;
} 